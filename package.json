{
  "name": "@twoquarterrican/expand",
  "version": "0.0.2",
  "description": "Recursively expand values",
  "main": "lib/index.js",
  "module": "lib/index.es5.js",
  "types": "lib/index.d.ts",
  "author": "twoquarterrican",
  "repository": {
    "type": "git",
    "url": "git+https://gitbug.com/twoquarterrican/expand"
  },
  "scripts": {
    "lint": "eslint ./src --max-warnings 0 --ext .tsx,.ts,.js && prettier --check src",
    "prettier": "prettier --write src",
    "build": "npm run lint && npm run test && npm run compile",
    "clean": "rimraf lib/",
    "compile": "npm run clean && tsc",
    "rimraf": "./node_modules/rimraf/bin.js",
    "test": "jest --coverage",
    "prepare": "npm run compile",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "expand",
    "configuration"
  ],
  "license": "MIT",
  "homepage": "https://github.com/twoquarterrican/expand#readme",
  "bugs": {
    "url": "https://github.com/twoquarterrican/expand/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "auto",
    "jsxSingleQuote": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "recursive-reducer": "0.0.5"
  }
}
